{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "/**.csproj"          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "namespaceLayout": "flattened"    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"      ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"        ],
        "exclude": [
          "obj/**",
          "_site/**"        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "home"    ],
    "postProcessors": [],
    "cleanupCacheHistory": true,
    "noLangKeyword": false,
    "keepFileLink": false,
    "disableGitFeatures": false,
    "_appFooter": "<span>Copyright &copy; <script>document.write(new Date().getFullYear())</script> pinDeliver AB</span>"    }
  }
}
