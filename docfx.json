{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "/**.csproj"          ]
        }
      ],
      "dest": "dest",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "namespaceLayout": "flattened"    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "/articles/**.md",
          "/articles/**/toc.yml",
          "*.md",
          "index.md"        ]
      },
      {
        "files": [
          "toc.yml",
          "/articles/**.md",
          "/articles/**/toc.yml",
          "*.md",
          "index.md"        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "logo.svg",
          "favicon.ico",
          "styles/*.css",
          "styles/*.js"     ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"        ],
        "exclude": [
          "obj/**",
          "_site/**"        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"    ],
    "postProcessors": [],
    "cleanupCacheHistory": true,
    "noLangKeyword": false,
    "keepFileLink": false,
    "disableGitFeatures": false,
    "_appFooter": "<span>Copyright &copy; <script>document.write(new Date().getFullYear())</script> pinDeliver AB</span>"    }
  }
}
